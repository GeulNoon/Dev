from konlpy.tag import Kkma
kkma = Kkma()
morph_list=[]
for i in txt:
 morphs = kkma.morphs(i)

 for morph in morphs:
   morph_list.append(morph)
morph_list
from collections import Counter
count = Counter(morph_list)
count
count.most_common()
for i in morph_list:
  f=open('kobartkkmamorph.txt','a',encoding='cp949')
  f.write(i+'\n')
  f.close()
